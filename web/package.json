{
  "name": "moku",
  "version": "0.64.3",
  "type": "module",
  "description": "Web UI for launching and interacting with Claude Code agents",
  "license": "MIT",
  "author": "Moku",
  "bin": {
    "moku": "./bin/cli.ts"
  },
  "files": [
    "bin/",
    "server/",
    "dist/"
  ],
  "engines": {
    "bun": ">=1.0.0"
  },
  "keywords": [
    "claude",
    "claude-code",
    "ai",
    "agent",
    "web-ui",
    "vibe-coding"
  ],
  "scripts": {
    "dev": "bun dev.ts",
    "dev:api": "bun --watch server/index.ts",
    "dev:vite": "vite",
    "build": "vite build",
    "start": "NODE_ENV=production bun server/index.ts",
    "prepublishOnly": "bun run build",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:a11y": "vitest run --testNamePattern='axe accessibility|accessible names|aria-expanded'",
    "test:codex-contract": "vitest run server/codex-protocol-*.test.ts",
    "generate-token": "bun bin/generate-token.ts"
  },
  "dependencies": {
    "@base-ui/react": "^1.2.0",
    "@codemirror/lang-cpp": "^6.0.3",
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.11",
    "@codemirror/lang-java": "^6.0.2",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/lang-markdown": "^6.5.0",
    "@codemirror/lang-python": "^6.2.1",
    "@codemirror/lang-rust": "^6.0.2",
    "@codemirror/lang-sql": "^6.10.0",
    "@codemirror/lang-xml": "^6.1.0",
    "@codemirror/lang-yaml": "^6.1.2",
    "@codemirror/view": "^6.39.15",
    "@fontsource-variable/plus-jakarta-sans": "^5.2.8",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "@uiw/react-codemirror": "^4.25.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "croner": "^10.0.1",
    "diff": "^8.0.3",
    "fzf": "^0.5.2",
    "hono": "^4.7.0",
    "lucide-react": "^0.575.0",
    "motion": "^12.34.3",
    "posthog-js": "^1.347.2",
    "qrcode": "^1.5.4",
    "shiki": "^4.0.0",
    "sonner": "^2.0.7",
    "streamdown": "^2.3.0",
    "tailwind-merge": "^3.5.0",
    "tw-animate-css": "^1.4.0",
    "use-stick-to-bottom": "^1.1.3",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "^1.2.5",
    "@types/diff": "^8.0.0",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.4.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/xterm": "^6.0.0",
    "autoprefixer": "^10.4.21",
    "jsdom": "^28.0.0",
    "postcss": "^8.5.3",
    "react": "^19.0.0",
    "react-arborist": "^3.4.3",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^4.6.2",
    "remark-gfm": "^4.0.1",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.9.3",
    "vite": "^6.3.0",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.18",
    "vitest-axe": "^0.1.0",
    "zustand": "^5.0.0"
  }
}
